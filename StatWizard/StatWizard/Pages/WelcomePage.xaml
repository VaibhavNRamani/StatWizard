<Page x:Class="StatWizard.Pages.WelcomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:StatWizard.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="WelcomePage">

    <Grid Background="White">
        <TextBlock x:Name="WelcomeToStatWizardTextBlock" HorizontalAlignment="Center" Height="21" TextWrapping="Wrap" Text="Welcome to StatWizard" VerticalAlignment="Top" Width="124" Margin="0,177,0,0"/>
        <Button x:Name="StartButton" Content="Start" HorizontalAlignment="Center" Height="21" Margin="0,204,0,0" VerticalAlignment="Top" Width="64" Click="StartButton_Click"/>

    </Grid>
</Page>
